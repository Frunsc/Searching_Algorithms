#include <stdio.h>

int main()
{
    int sayi;
    int array[] = {2,3,7,9,11,12,15,30,36,38,41,45,49,57,68};
    printf("{2,3,7,9,11,12,15,30,36,38,41,45,49,57,68} dizisinden sirasini ogrenmek istediginiz sayiyi seciniz.\n");
    scanf("%d",&sayi);
    for(int i=0;i <= sayi;i++)
    {
        if (array[i] == sayi) printf("%d. sirada",++i);

    }

}
